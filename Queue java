import java.util.LinkedList;
import java.util.Queue;

public class Main {
    public static void main(String[] args) {
        Queue<Integer> queue = new LinkedList<>();

        // Menambahkan elemen ke queue
        queue.add(1);
        queue.add(2);
        queue.add(3);

        // Menampilkan elemen paling depan dari queue
        System.out.println("Elemen paling depan: " + queue.peek());

        // Menghapus elemen paling depan dari queue
        queue.remove();

        // Menampilkan elemen paling depan setelah dihapus
        System.out.println("Elemen paling depan setelah dihapus: " + queue.peek());
    }
}
